{
  "compilerOptions": {
    "target": "ES2015",
    "module": "commonjs",
    "resolveJsonModule": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["*"],
      "@fixtures/*": ["framework/fixtures/*"],
      "@models/*": ["framework/models/*"],
      "@utils/*": ["framework/utils/*"],
      "@test-data/*": ["test-data/*"]
    }
  },
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Recommended"
}
